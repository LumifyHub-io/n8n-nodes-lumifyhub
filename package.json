{
  "name": "n8n-nodes-lumifyhub",
  "version": "0.1.1",
  "description": "n8n community node for LumifyHub - Create and manage pages in your LumifyHub workspace from any workflow",
  "license": "MIT",
  "homepage": "https://lumifyhub.io",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "lumifyhub",
    "workspace",
    "automation",
    "productivity",
    "pages"
  ],
  "author": {
    "name": "Saad",
    "email": "saadnvd2017@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LumifyHub-io/n8n-nodes-lumifyhub.git"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease",
    "prepare": "chmod +x .githooks/pre-commit && cp .githooks/pre-commit .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/LumifyhubApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Lumifyhub/Lumifyhub.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "eslint": "9.32.0",
    "n8n-workflow": "^1.114.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
